@model DataObjects.BrowseProductViewModel

<td>
    @Html.DisplayFor(modelItem => modelItem.Name)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.Description)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.PriceString)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.Supplier_Name)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.CategoryID)
</td>
<td>
        <img src="@Url.Action("PictureOf", "Products", new { id = Model.ProductId})" width="100" height="100" />
</td>
<td>
    @Html.ActionLink("Details", "Details", "Products", new { id = Model.ProductId, supplierId = Model.SupplierID }, null)
</td>

