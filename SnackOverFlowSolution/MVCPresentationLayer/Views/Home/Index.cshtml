@{
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}


<!-- Start main-header -->
<header class="main-header slider-on" id="top">
    <div class="main-slider-container">
        <div class="main-slider">
            <ul class="scrollme">
                <li data-transition="fade" data-slotamount="7" data-masterspeed="1500" data-title="Chana Experience">
                    <img class="rs-parallaxlevel-1" src="~/Images/Background/bg-1.jpg" alt="Marine Slider BG" data-bgfit="cover" data-bgposition="left top" data-bgrepeat="no-repeat">
                    <div class="tp-caption customin customout" id="topOne">
                        <img src="~/Images/logo.png" alt="Organic" class="animateme" id="sOne">
                    </div>
                </li>
                <li data-transition="fadetorightfadetoleft" data-slotamount="7" data-masterspeed="1500" data-title="Coffee Experience">
                    <img class="rs-parallaxlevel-1" src="~/Images/background/bg-2.jpg" alt="Marine Slider BG" data-bgfit="cover" data-bgposition="left top" data-bgrepeat="no-repeat">
                    <div class="tp-caption customin customout" id="topTwo">
                        <img src="~/Images/logo.png" alt="Organic" class="animateme" id="sTwo">
                    </div>
                </li>
            </ul>
        </div><!-- /main-slider -->
    </div><!-- /main-slider-container -->
</header>
<!-- End main-header -->

@*Slider*@
<section class="no-padding">
    <div class="double-view">
        <div class="row">
            <div class="col-md-6">
                <div class="image-container wow fadeInLeft">
                    <figure>
                        <img src="~/Images/Background/custom-bg30.jpg" alt="About Gourmet">
                    </figure>
                </div><!-- /image-container -->
            </div>
            <div class="col-md-4">
                <div class="contents-container wow fadeInRight">
                    <header class="section-title style3 color-white">
                        <h1><span>About</span> Us</h1>
                        <img src="~/Images/slider-images/Flogo2.png" alt="About Snack Overflow">
                        <p>
                            Snack Overflow is a startup company with the desire to connect produce consumers with the
                            local suppliers. The company believes this is an important step to facilitate a more economical and self-
                            sustaining organic produce market. Snack Overflow also believes its mission will boost the local
                            economy, keeping products sold and purchased as locally connected as possible.
                        </p>
                    </header>
                </div><!-- /contents-container -->
            </div><!-- /col-md-6 -->
        </div>
    </div>
</section>
@*End of Slider*@

@*Gallery*@
<section class="gallery style3 white-bg">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2 wow fadeInDown">
                <header class="section-title style3">
                    <h1><span>Our</span> Gallery</h1>
                    <p>View... </p>
                </header>
            </div><!-- /col-md-8 -->
        </div><!-- /row -->
    </div><!-- /container -->
    <div class="gallery-items-container wow fadeInDown">
        <ul class="clearfix">
            <li class="overlay-container mix vegetarian">
                <img src="~/Images/Gallery/gallery-1.jpg" height="214" width="546" alt="avocado">
                <div class="overlay">
                    <div class="overlay-details">
                        <h3>Freshly grown avocado</h3>
                    </div><!-- /overlay-details -->
                </div><!-- /overlay -->
            </li>
            <li class="overlay-container mix  desert">
                <img src="~/Images/Gallery/gallery-2.jpg" height="214" width="546" alt="eggs">
                <div class="overlay">
                    <div class="overlay-details">
                        <h3>Organic Chicken Eggs</h3>
                    </div><!-- /overlay-details -->
                </div><!-- /overlay -->
            </li>
            <li class="overlay-container mix  vegetarian">
                <img src="~/Images/Gallery/gallery-3.jpg" height="214" width="546" alt="pepper">
                <div class="overlay">
                    <div class="overlay-details">
                        <h3>Habanero Peppers</h3>
                    </div><!-- /overlay-details -->
                </div><!-- /overlay -->
            </li>
            <li class="overlay-container mix meat">
                <img src="~/Images/Gallery/gallery-4.jpg" height="214" width="546" alt="fish">
                <div class="overlay">
                    <div class="overlay-details">
                        <h3>Fish</h3>
                    </div><!-- /overlay-details -->
                </div><!-- /overlay -->
            </li>
            <li class="overlay-container mix desert">
                <img src="~/Images/Gallery/gallery-5.jpg" height="214" width="546" alt="cuisinepowders">
                <div class="overlay">
                    <div class="overlay-details">
                        <h3>Cuisine Powders</h3>
                    </div><!-- /overlay-details -->
                </div><!-- /overlay -->
            </li>
        </ul>
    </div><!-- /gallery-items-container -->
</section><!-- /gallery -->
@*End of Gallery*@

@*Footer*@
<footer class="main-footer style3">
    <div class="container">
        <div class="row">
            <div class="col-md-10 col-md-offset-1 wow fadeInDown">
                <div class="contact-form-contaienr">
                    <div class="section-title style3">
                        <h1><span>Get</span> In Touch</h1>
                    </div>
                    <form id="contact-form" method="post" action="#">
                        <div class="input-container col-md-6">
                            <input type="text" id="name" name="name" placeholder="Name*" required>
                        </div>
                        <div class="input-container col-md-6">
                            <input type="email" id="email" name="email" placeholder="Email*" required>
                        </div>
                        <div class="input-container col-md-12">
                            <textarea id="message" name="message" rows="4" placeholder="Message" required></textarea>
                        </div>
                        <div class="input-container col-md-12">
                            <button type="submit">Send Message</button>
                        </div>
                    </form>
                    <div id="form-messages"></div>
                </div><!-- /contact-form-container -->
            </div><!-- /col-md-10 -->
        </div><!-- /row -->
    </div><!-- /container -->
    
    @*Actual Footer*@
    <div class="bottom-bar">
        @Html.Partial("_FooterPartial")
    </div>
    @*End of Actual Footer*@

</footer>
@*End of Footer*@